{
  "name": "next-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "node app/server.js",
    "build": "next build ./app",
    "start": "NODE_ENV=production node app/server.js",
    "export": "yarn pre-export && yarn build ./app && next export ./app",
    "pre-export": "yarn lint && yarn lint:scss",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:scss": "sass-lint -v -q",
    "lint-staged": "lint-staged"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "sass-lint -v -q"
    ]
  },
  "sasslintConfig": "./.sass-lint-config.yml",
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0-rc.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "dotenv": "^5.0.1",
    "express": "^4.14.0",
    "next": "^7.0.1",
    "next-routes": "^1.4.2",
    "node-sass": "^4.9.3",
    "postcss-cssnext": "^3.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "superior-scss": "^3.0.2"
  },
  "devDependencies": {
    "@ueno/eslint-config": "^1.2.8",
    "eslint": "^4.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "lint-staged": "^7.0.0",
    "pre-commit": "^1.2.2",
    "sass-lint": "^1.12.1"
  },
  "license": "UNLICENSED"
}
